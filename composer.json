{
  "name": "camunda/camunda-bpm-php-sdk",
  "description": "PHP SDK for camundaBPM process services",
  "license": "Apache-2.0",
  "minimum-stability": "stable",
  "keywords": ["sdk","camunda", "camunda bpm"],
  "homepage": "http://camunda.org",
  "authors": [
    {
      "name": "Stefan Hentschel",
      "email": "stefan.hentschel@camunda.com"
    }
  ],

  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*"
  },

  "require-dev": {
    "phpunit/phpunit": "^8"
  },

  "archive": {
    "exclude": [
      "/procedual/test",
      "/procedual/test/*",
      "/oop/test",
      "/oop/test/*"
    ]
  },

  "autoload": {
    "psr-4": {
      "org\\camunda\\php\\sdk": "src/"
    },
    "classmap": [
      "oop/main/"
    ]
  }
}
